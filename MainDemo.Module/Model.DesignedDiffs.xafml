  <Validation>
    <Rules>
      <!--Contact-->
      <RuleRequiredField Id="Contact_EmailIsNotEmpty" TargetType="MainDemo.Module.BusinessObjects.Contact" TargetPropertyName="Email" TargetContextIDs="Save" IsNewNode="True" />
      <RuleRequiredField Id="Contact_FirstNameIsNotEmpty" TargetType="MainDemo.Module.BusinessObjects.Contact" TargetPropertyName="FirstName" TargetContextIDs="Save" IsNewNode="True" />
      <RuleRequiredField Id="Contact_LastNameIsNotEmpty" TargetType="MainDemo.Module.BusinessObjects.Contact" TargetPropertyName="LastName" TargetContextIDs="Save" IsNewNode="True" />
      <RuleRange Id="Contact_InvalidBirthday" TargetType="MainDemo.Module.BusinessObjects.Contact" TargetContextIDs="Save" TargetPropertyName="Birthday" MaximumValueExpression="Today()" MinimumValueExpression="#1900-01-01 00:00:00.00000#" CustomMessageTemplate="&quot;Birth Date&quot; must be between 1/1/1900 and today." IsNewNode="True" />
      <RuleRange Id="Contact_InvalidWeddingDate" TargetType="MainDemo.Module.BusinessObjects.Contact" TargetContextIDs="Save" TargetPropertyName="Anniversary" MaximumValueExpression="Today()" MinimumValueExpression="#1900-01-01 00:00:00.00000#" CustomMessageTemplate="&quot;Wedding Date&quot; must be between 1/1/1900 and today." IsNewNode="True" />
      <RuleRegularExpression Id="Contact_InvalidEmail" Pattern="^([\w\.\-]+)@([\w\-]+)((\.(\w){2,3})+)$" TargetContextIDs="Save" TargetPropertyName="Email" TargetType="MainDemo.Module.BusinessObjects.Contact" CustomMessageTemplate="Invalid &quot;Email&quot;." IsNewNode="True" />
      <RuleRegularExpression Id="Contact_PhoneNumber" Pattern="^\([0-9]{3}\)\s?[0-9]{3}-[0-9]{4}$" TargetContextIDs="Save" TargetPropertyName="Number" TargetType="DevExpress.Persistent.BaseImpl.PhoneNumber" CustomMessageTemplate="&quot;Number&quot; must be in the following format: &quot;(000) 000-0000&quot;." IsNewNode="True" />
      <RuleRequiredField Id="Contact_PhoneNumberIsNotEmpty" TargetType="DevExpress.Persistent.BaseImpl.PhoneNumber" TargetPropertyName="Number" TargetContextIDs="Save" IsNewNode="True" />
      <!--Task-->
      <RuleRange Id="Task_PercentCompleted" TargetType="MainDemo.Module.BusinessObjects.DemoTask" TargetContextIDs="Save" TargetPropertyName="PercentCompleted" MinimumValue="0" MaximumValue="100" IsNewNode="True" />
      <RuleRequiredField Id="TaskSubjectIsNotEmpty" TargetType="DevExpress.Persistent.BaseImpl.Task" TargetPropertyName="Subject" TargetContextIDs="Save" IsNewNode="True" />
      <RuleValueComparison Id="Task_StartDate_DueDate" TargetPropertyName="DueDate" RightOperandExpression="StartDate" OperatorType="GreaterThanOrEqual" TargetType="MainDemo.Module.BusinessObjects.DemoTask" TargetContextIDs="Save" IsNewNode="True" />
      <!--Calendar-->
      <RuleRequiredField Id="Calendar_SubjectIsNotEmpty" TargetType="DevExpress.Persistent.BaseImpl.Event" TargetPropertyName="Subject" TargetContextIDs="Save" IsNewNode="True" />
    </Rules>
  </Validation>
